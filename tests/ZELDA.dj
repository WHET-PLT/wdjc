	note as4_4 = note (ASharp4, 100, quarter);
	note f3_6 = note (F3, 100, quarter + eighth);
	note as4_1 = note (ASharp4, 100, sixteenth);
	note c4_1 = note (C4, 100, sixteenth);
	note d4_1 = note (D4, 100, sixteenth);
	note ds4_1 = note (DSharp4, 100, sixteenth);

	
	/* CHORDING THEM OUT */
	chord c1 = chord (as4_10);
	chord c2 = chord (f3_2);
	/* chord c2b = chord (f3_2); */
