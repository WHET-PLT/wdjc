	chord c12 = chord (as4_1);
	chord c13 = chord (c4_1);
	chord c14 = chord (d4_1);
	chord c15 = chord (ds4_1);
		
	loop (2) {
		treble_clef = treble_clef . c1;
		treble_clef = treble_clef . c2;
		/* treble_clef = treble_clef . c2b; */
