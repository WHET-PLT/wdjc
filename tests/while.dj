/* Test for if statement */

song score () 
{
	
	double i;
	double j;

	i = 0;
	j = 1;

	/* ID < LIT */
	while( i < 1)
	{
		i = i + 1;
	}

	/* ID > LIT */
	while( 1 > i)
	{
		i = i + 1;
	}
	
	/* ID == ID */
	while( 0 == 1 )
	{
		i = i + 1;
	}

	/* LIT != LIT */
	while( i != j )
	{
		i = i + 1;
	}
	return 1;

}